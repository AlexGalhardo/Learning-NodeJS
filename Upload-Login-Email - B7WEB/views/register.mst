{{> header}} 

<h1>Registre sua conta</h1>

<form method="POST">
	<label>
		Nome:
		<input type="text" name="name">
	</label>
	<br>
	<label>
		Email:
		<input type="email" name="email">
	</label>
	<br>
	<label>
		Senha:
		<input type="password" name="password">
	</label>
	<br>
	<label>
		<input type="submit" value="Registrar">
	</label>
</form>