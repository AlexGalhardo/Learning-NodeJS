{{> header}} 

<h1>Mude sua senha</h1>

<form method="POST">
	<label>
		Nova Senha:
		<input type="password" name="password">
	</label>
	<br>

	<label>
		Confirmar Nova Senha:
		<input type="password" name="password-confirm">
	</label>
	<br>

	<label>
		<input type="submit" value="Alterar Senha">
	</label>
</form>